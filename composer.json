{
    "name": "wedevelop/phpstan-class-flags",
    "description": "A PHPStan extension to enforce explicitly stating when a class should not have readonly or final keywords.",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "WeDevelop\\PHPStan\\ClassFlags\\": "src/"
        },
        "files": ["bootstrap.php"]
    },
    "authors": [
        {
            "name": "Zan Baldwin",
            "email": "hello@zanbaldwin.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "phpstan/phpstan": "^1.0"
    },
    "require-dev": {
        "phpstan/phpstan-deprecation-rules": "^1.1"
    },
    "extra": {
        "phpstan": {
            "includes": [
                "rules.neon"
            ]
        }
    }
}
